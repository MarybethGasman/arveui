{
  "name": "arveui",
  "version": "0.0.20",
  "description": "Vue Component Library",
  "author": {
    "name": "tannal",
    "email": "tannal.cn@gmail.com"
  },
  "license": "Apache-2.0",
  "homepage": "https://www.npmjs.com/package/arveui",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarybethGasman/arveui"
  },
  "keywords": [
    "vue",
    "vue components",
    "vue framework",
    "ui framework",
    "ui library",
    "components",
    "component framework",
    "component library"
  ],
  "main": "./dist/arveui.cjs.js",
  "module": "./dist/arveui.es.js",
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/arveui.cjs.js",
      "require": "./dist/arveui.es.js"
    }
  },
  "browser": {
    ".": "./dist/arveui.es.js"
  },
  "types": "./dist/ViewerPlugin.d.ts",
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "test": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@vitejs/plugin-vue": "^2.3.4",
    "rollup-plugin-typescript2": "^0.33.0",
    "typescript": "^4.5.4",
    "vite": "^2.9.15",
    "vue-tsc": "^0.40.5"
  }
}
